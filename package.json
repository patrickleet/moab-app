    "fela-plugin-validator": "^1.1.0",
    "graphql": "^0.6.1",
    "graphql-tag": "^0.1.7",
    "graphql-tools": "^0.6.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.2",
    "react-addons-update": "^15.2.1",
    "react-apollo": "^0.4.4",
    "react-debounce-input": "^2.4.0",
    "react-dom": "^15.0.2",
    "react-fela": "^2.1.0",
    "react-ga": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",

    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-addons-update": "^15.2.1",
    "react-apollo": "^2.0.4",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.2.0",
    "react-fela": "^7.0.1",
    "react-ga": "^2.1.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",

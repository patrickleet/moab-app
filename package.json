{
  "name": "app",
  "version": "0.1.0",
  "description": "app boilerplate with react web and native, redux, apollo, ssr, webpack, code splitting",
  "main": "src/index.js",
  "scripts": {
    "start-app-client": "webpack-dev-server -d --hot --inline --config ./webpack/webpack.config.js --port 3020 --colors --no-info",
    "start-app-server": "nodemon src/ui/server.js --watch src/ui --exec babel-node",
    "start": "concurrently \"npm run start-app-client\" \"npm run start-app-server\"",
    "lint": "eslint src",
    "build": "webpack -p --progress --config ./webpack.production.config.js",
    "test": "npm run lint"
  },
  "author": "Patrick Scott",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^4.1.2",
    "concurrently": "^3.1.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "json-loader": "^0.5.4",
    "mocha": "^5.0.4",
    "nodemon": "^1.11.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "apollo-client": "^2.2.6",
    "autobind-decorator": "^2.1.0",
    "bunyan": "^1.8.1",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fela": "^6.1.4",
    "fela-beautifier": "^5.0.15",
    "fela-font-renderer": "^5.0.10",
    "fela-perf": "^5.0.3",
